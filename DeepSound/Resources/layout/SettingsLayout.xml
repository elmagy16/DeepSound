<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:orientation="vertical"
	android:background="?attr/colorPrimary"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	<androidx.appcompat.widget.Toolbar
		android:id="@+id/toolbar"
		android:layout_width="match_parent"
		android:layout_height="48dp"
		app:theme="@style/ToolbarTheme"
		android:layout_gravity="top"
		app:layout_collapseMode="pin"
		app:titleTextAppearance="@style/Toolbar.TitleText"
		android:background="@drawable/linear_gradient_drawable"
		android:elevation="2dp"   />

	<androidx.core.widget.NestedScrollView
		android:id="@+id/nested_scroll_view"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:scrollbars="none"
		android:scrollingCache="true">
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			android:padding="10dp">

			<View
				android:layout_width="match_parent"
				android:layout_height="20dp" />

			<RelativeLayout
				android:layout_width="match_parent"
				android:layout_height="95dp"
				android:id="@+id/info_container"
				android:background="?attr/selectableItemBackground"
				android:gravity="center"
				android:layout_gravity="center"
				android:clickable="false"
				android:focusable="false"
				android:orientation="vertical"
				android:layout_marginStart="25dp"
				android:layout_marginEnd="25dp">
				<androidx.cardview.widget.CardView
					android:id="@+id/cardview2"
					android:layout_gravity="fill"
					android:layout_width="120dp"
					android:layout_height="95dp"
					app:cardCornerRadius="10dp"
					android:layout_marginBottom="0dp"
					app:cardElevation="0dp">
					<ImageView
						android:id="@+id/image"
						android:scaleType="centerCrop"
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:focusable="false"
						android:clickable="false"
						android:src="@drawable/no_profile_image"
						android:focusableInTouchMode="false" />
				</androidx.cardview.widget.CardView>
				<LinearLayout
					android:layout_toEndOf="@+id/cardview2"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:clickable="false"
					android:orientation="vertical"
					android:paddingStart="0dp"
					android:paddingEnd="7dp"
					android:layout_marginTop="5dp"
					android:layout_marginEnd="11dp"
					android:layout_marginStart="10dp"
					android:focusable="false"
					android:layout_gravity="center_horizontal"
					android:layout_weight="3">
					<LinearLayout
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/Name"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:ellipsize="none"
							android:lines="1"
							android:text="Wael anjo"
							android:textSize="17dp"
							android:textStyle="bold"
							android:layout_marginTop="5dp" />
						<ImageView
							android:id="@+id/IconVerified"
							android:layout_height="wrap_content"
							android:src="@drawable/icon_checkmark_small_vector"
							android:layout_width="wrap_content"
							android:layout_gravity="center"
							android:layout_marginStart="5dp"
							android:layout_marginTop="3dp" />
					</LinearLayout>

					<LinearLayout
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:orientation="vertical">
						<TextView
							android:id="@+id/username"
							android:layout_height="wrap_content"
							android:textSize="12dp"
							android:layout_width="wrap_content"
							android:layout_gravity="center" />
					</LinearLayout>
				</LinearLayout>
			</RelativeLayout>

			<View
				android:layout_width="match_parent"
				android:layout_height="20dp" />

			<androidx.cardview.widget.CardView
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				app:cardBackgroundColor="?attr/colorFill"
				app:cardCornerRadius="20dp"
				app:cardElevation="2dp"
				app:cardUseCompatPadding="true">
				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:orientation="vertical"
					android:paddingBottom="10dp"
					android:paddingTop="10dp">
					<FrameLayout
						android:id="@+id/layoutEditProfile"
						android:layout_width="match_parent"
						android:layout_height="50dp"
						android:background="?attr/selectableItemBackground"
						android:clickable="true"
						android:gravity="center_vertical"
						android:orientation="horizontal"
						android:paddingStart="20dp"
						android:paddingEnd="20dp">
						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="@string/Lbl_EditProfileInfo"
							android:textSize="16sp"
							android:textStyle="normal"
							android:layout_gravity="start|center" />
						<ImageView
							android:layout_width="25dp"
							android:layout_height="25dp"
							android:src="@xml/ic_arrow_right"
							android:tint="?attr/colorSecondaryExtra"
							android:padding="2dp"
							android:layout_gravity="end|center" />
					</FrameLayout>
					<View
						android:layout_width="match_parent"
						android:layout_height="2dp"
						android:background="?attr/colorDivider"/>
					<FrameLayout
						android:id="@+id/layoutMyAccount"
						android:layout_width="match_parent"
						android:layout_height="50dp"
						android:background="?attr/selectableItemBackground"
						android:clickable="true"
						android:gravity="center_vertical"
						android:orientation="horizontal"
						android:paddingStart="20dp"
						android:paddingEnd="20dp">
						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="@string/Lbl_MyAccount"
							android:textSize="16sp"
							android:textStyle="normal"
							android:layout_gravity="start|center" />
						<ImageView
							android:layout_width="25dp"
							android:layout_height="25dp"
							android:src="@xml/ic_arrow_right"
							android:tint="?attr/colorSecondaryExtra"
							android:padding="2dp"
							android:layout_gravity="end|center" />
					</FrameLayout>
					<View
						android:layout_width="match_parent"
						android:layout_height="2dp"
						android:background="?attr/colorDivider"/>
					<FrameLayout
						android:id="@+id/layoutNotifications"
						android:layout_width="match_parent"
						android:layout_height="50dp"
						android:background="?attr/selectableItemBackground"
						android:clickable="true"
						android:gravity="center_vertical"
						android:orientation="horizontal"
						android:paddingStart="20dp"
						android:paddingEnd="20dp">
						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="@string/Lbl_Notifications"
							android:textSize="16sp"
							android:textStyle="normal"
							android:layout_gravity="start|center" />
						<ImageView
							android:layout_width="25dp"
							android:layout_height="25dp"
							android:src="@xml/ic_arrow_right"
							android:tint="?attr/colorSecondaryExtra"
							android:padding="2dp"
							android:layout_gravity="end|center" />
					</FrameLayout>
					<View
						android:layout_width="match_parent"
						android:layout_height="2dp"
						android:background="?attr/colorDivider"/> 
					<FrameLayout
						android:id="@+id/layoutGoPro"
						android:layout_width="match_parent"
						android:layout_height="50dp"
						android:background="?attr/selectableItemBackground"
						android:clickable="true"
						android:gravity="center_vertical"
						android:orientation="horizontal"
						android:paddingStart="20dp"
						android:paddingEnd="20dp">
						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="@string/Lbl_Go_Pro"
							android:textSize="16sp"
							android:textStyle="normal"
							android:layout_gravity="start|center" />
						<ImageView
							android:layout_width="25dp"
							android:layout_height="25dp"
							android:src="@xml/ic_arrow_right"
							android:tint="?attr/colorSecondaryExtra"
							android:padding="2dp"
							android:layout_gravity="end|center" />
					</FrameLayout> 
					<View
						android:layout_width="match_parent"
						android:layout_height="2dp"
						android:background="?attr/colorDivider"/>
					<FrameLayout
						android:id="@+id/layoutBlockedUsers"
						android:layout_width="match_parent"
						android:layout_height="50dp"
						android:background="?attr/selectableItemBackground"
						android:clickable="true"
						android:gravity="center_vertical"
						android:orientation="horizontal"
						android:paddingStart="20dp"
						android:paddingEnd="20dp">
						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="@string/Lbl_BlockedUsers"
							android:textSize="16sp"
							android:textStyle="normal"
							android:layout_gravity="start|center" />
						<ImageView
							android:layout_width="25dp"
							android:layout_height="25dp"
							android:src="@xml/ic_arrow_right"
							android:tint="?attr/colorSecondaryExtra"
							android:padding="2dp"
							android:layout_gravity="end|center" />
					</FrameLayout>
				</LinearLayout>
			</androidx.cardview.widget.CardView>
		
			
			<View
				android:layout_width="match_parent"
				android:layout_height="20dp" />
			 
			<androidx.cardview.widget.CardView
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				app:cardBackgroundColor="?attr/colorFill"
				app:cardCornerRadius="20dp"
				app:cardElevation="2dp"
				app:cardUseCompatPadding="true">
				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:orientation="vertical"
					android:paddingBottom="10dp"
					android:paddingTop="10dp">
					  
					<FrameLayout
						android:id="@+id/layoutMyAddresses"
						android:layout_width="match_parent"
						android:layout_height="50dp"
						android:background="?attr/selectableItemBackground"
						android:clickable="true"
						android:visibility="gone"
						android:gravity="center_vertical"
						android:orientation="horizontal"
						android:paddingStart="20dp"
						android:paddingEnd="20dp">
						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="@string/Lbl_MyAddresses"
							android:textSize="16sp"
							android:textStyle="normal"
							android:layout_gravity="start|center" />
						<ImageView
							android:layout_width="25dp"
							android:layout_height="25dp"
							android:src="@xml/ic_arrow_right"
							android:tint="?attr/colorSecondaryExtra"
							android:padding="2dp"
							android:layout_gravity="end|center" />
					</FrameLayout>
					<View
						android:layout_width="match_parent"
						android:visibility="gone"
						android:layout_height="2dp"
						android:background="?attr/colorDivider"/>
					<FrameLayout
						android:id="@+id/layoutWithdrawals"
						android:layout_width="match_parent"
						android:layout_height="50dp"
						android:background="?attr/selectableItemBackground"
						android:clickable="true"
						android:gravity="center_vertical"
						android:orientation="horizontal"
						android:paddingStart="20dp"
						android:paddingEnd="20dp">
						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="@string/Lbl_Withdrawals"
							android:textSize="16sp"
