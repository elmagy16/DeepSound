					</FrameLayout>
					<View
						android:layout_width="match_parent"
						android:layout_height="2dp"
						android:background="?attr/colorDivider"/>
					<FrameLayout
						android:id="@+id/layoutWallet"
						android:layout_width="match_parent"
						android:layout_height="50dp"
						android:background="?attr/selectableItemBackground"
						android:clickable="true"
						android:gravity="center_vertical"
						android:orientation="horizontal"
						android:paddingStart="20dp"
						android:paddingEnd="20dp">
						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="@string/Lbl_Wallet"
							android:textSize="16sp"
							android:textStyle="normal"
							android:layout_gravity="start|center" />
						<ImageView
							android:layout_width="25dp"
							android:layout_height="25dp"
							android:src="@xml/ic_arrow_right"
							android:tint="?attr/colorSecondaryExtra"
							android:padding="2dp"
							android:layout_gravity="end|center" />
					</FrameLayout> 
				</LinearLayout>
			</androidx.cardview.widget.CardView>
		 
			<View
				android:layout_width="match_parent"
				android:layout_height="20dp" />
			 
			<androidx.cardview.widget.CardView
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				app:cardBackgroundColor="?attr/colorFill"
				app:cardCornerRadius="20dp"
				app:cardElevation="2dp"
				app:cardUseCompatPadding="true">
				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:orientation="vertical"
					android:paddingBottom="10dp"
					android:paddingTop="10dp">
					<FrameLayout
						android:id="@+id/layoutPassword"
						android:layout_width="match_parent"
						android:layout_height="50dp"
						android:background="?attr/selectableItemBackground"
						android:clickable="true"
						android:gravity="center_vertical"
						android:orientation="horizontal"
						android:paddingStart="20dp"
						android:paddingEnd="20dp">
						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="@string/Lbl_Password"
							android:textSize="16sp"
							android:textStyle="normal"
							android:layout_gravity="start|center" />
						<ImageView
							android:layout_width="25dp"
							android:layout_height="25dp"
							android:src="@xml/ic_arrow_right"
							android:tint="?attr/colorSecondaryExtra"
							android:padding="2dp"
							android:layout_gravity="end|center" />
					</FrameLayout>
					<View
						android:layout_width="match_parent"
						android:layout_height="2dp"
						android:background="?attr/colorDivider"/>
					<FrameLayout
						android:id="@+id/layoutTwoFactor"
						android:layout_width="match_parent"
						android:layout_height="50dp"
						android:background="?attr/selectableItemBackground"
						android:clickable="true"
						android:gravity="center_vertical"
						android:orientation="horizontal"
						android:paddingStart="20dp"
						android:paddingEnd="20dp">
						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="@string/Lbl_TwoFactor"
							android:textSize="16sp"
							android:textStyle="normal"
							android:layout_gravity="start|center" />
						<ImageView
							android:layout_width="25dp"
							android:layout_height="25dp"
							android:src="@xml/ic_arrow_right"
							android:tint="?attr/colorSecondaryExtra"
							android:padding="2dp"
							android:layout_gravity="end|center" />
					</FrameLayout>
					<View
						android:layout_width="match_parent"
						android:layout_height="2dp"
						android:background="?attr/colorDivider"/>
					<FrameLayout
						android:id="@+id/layoutManageSessions"
						android:layout_width="match_parent"
						android:layout_height="50dp"
						android:background="?attr/selectableItemBackground"
						android:clickable="true"
						android:gravity="center_vertical"
						android:orientation="horizontal"
						android:paddingStart="20dp"
						android:paddingEnd="20dp">
						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="@string/Lbl_ManageSessions"
							android:textSize="16sp"
							android:textStyle="normal"
							android:layout_gravity="start|center" />
						<ImageView
							android:layout_width="25dp"
							android:layout_height="25dp"
							android:src="@xml/ic_arrow_right"
							android:tint="?attr/colorSecondaryExtra"
							android:padding="2dp"
							android:layout_gravity="end|center" />
					</FrameLayout>
				</LinearLayout>
			</androidx.cardview.widget.CardView>
			<View
				android:layout_width="match_parent"
				android:layout_height="20dp" />

			<androidx.cardview.widget.CardView
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				app:cardBackgroundColor="?attr/colorFill"
				app:cardCornerRadius="20dp"
				app:cardElevation="2dp"
				app:cardUseCompatPadding="true">
				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:orientation="vertical"
					android:paddingBottom="10dp"
					android:paddingTop="10dp">
					<FrameLayout
						android:id="@+id/layoutTheme"
						android:layout_width="match_parent"
						android:layout_height="50dp"
						android:background="?attr/selectableItemBackground"
						android:clickable="true"
						android:gravity="center_vertical"
						android:orientation="horizontal"
						android:paddingStart="20dp"
						android:paddingEnd="20dp">
						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="@string/Lbl_Theme"
							android:textSize="16sp"
							android:textStyle="normal"
							android:layout_gravity="start|center" />
						<ImageView
							android:layout_width="25dp"
							android:layout_height="25dp"
							android:src="@xml/ic_arrow_right"
							android:tint="?attr/colorSecondaryExtra"
							android:padding="2dp"
							android:layout_gravity="end|center" />
					</FrameLayout>
				</LinearLayout>
			</androidx.cardview.widget.CardView>
			<View
				android:layout_width="match_parent"
				android:layout_height="20dp" />
			 
			<androidx.cardview.widget.CardView
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				app:cardBackgroundColor="?attr/colorFill"
				app:cardCornerRadius="20dp"
				app:cardElevation="2dp"
				app:cardUseCompatPadding="true">
				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:orientation="vertical"
					android:paddingBottom="10dp"
					android:paddingTop="10dp">

					<FrameLayout
						android:id="@+id/layoutInterest"
						android:layout_width="match_parent"
						android:layout_height="50dp"
						android:background="?attr/selectableItemBackground"
						android:clickable="true"
						android:gravity="center_vertical"
						android:orientation="horizontal"
						android:paddingStart="20dp"
						android:paddingEnd="20dp">
						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="@string/Lbl_Interest"
							android:textSize="16sp"
							android:textStyle="normal"
							android:layout_gravity="start|center" />
						<ImageView
							android:layout_width="25dp"
							android:layout_height="25dp"
							android:src="@xml/ic_arrow_right"
							android:tint="?attr/colorSecondaryExtra"
							android:padding="2dp"
							android:layout_gravity="end|center" />
					</FrameLayout>
				 
				</LinearLayout>
			</androidx.cardview.widget.CardView>
			<View
				android:layout_width="match_parent"
				android:layout_height="20dp" />
			 
			<androidx.cardview.widget.CardView
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				app:cardBackgroundColor="?attr/colorFill"
				app:cardCornerRadius="20dp"
				app:cardElevation="2dp"
				app:cardUseCompatPadding="true">
				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:orientation="vertical"
					android:paddingBottom="10dp"
					android:paddingTop="10dp">
					<FrameLayout
						android:id="@+id/layoutRateOurApp"
						android:layout_width="match_parent"
						android:layout_height="50dp"
						android:background="?attr/selectableItemBackground"
						android:clickable="true"
						android:gravity="center_vertical"
						android:orientation="horizontal"
						android:paddingStart="20dp"
						android:paddingEnd="20dp">
						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="@string/Lbl_RateOurApp"
							android:textSize="16sp"
							android:textStyle="normal"
							android:layout_gravity="start|center" />
						<ImageView
							android:layout_width="25dp"
							android:layout_height="25dp"
							android:src="@xml/ic_arrow_right"
							android:tint="?attr/colorSecondaryExtra"
							android:padding="2dp"
							android:layout_gravity="end|center" />
					</FrameLayout>
					<View
						android:layout_width="match_parent"
						android:layout_height="2dp"
						android:background="?attr/colorDivider"/>
					<FrameLayout
						android:id="@+id/layoutInviteFriends"
						android:layout_width="match_parent"
						android:layout_height="50dp"
						android:background="?attr/selectableItemBackground"
						android:clickable="true"
						android:gravity="center_vertical"
						android:orientation="horizontal"
						android:paddingStart="20dp"
						android:paddingEnd="20dp">
						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="@string/Lbl_InviteFriends"
							android:textSize="16sp"
							android:textStyle="normal"
							android:layout_gravity="start|center" />
						<ImageView
							android:layout_width="25dp"
							android:layout_height="25dp"
							android:src="@xml/ic_arrow_right"
							android:tint="?attr/colorSecondaryExtra"
							android:padding="2dp"
							android:layout_gravity="end|center" />
					</FrameLayout>
					<View
						android:layout_width="match_parent"
						android:layout_height="2dp"
						android:background="?attr/colorDivider"/>
					<FrameLayout
						android:id="@+id/layoutAboutApp"
						android:layout_width="match_parent"
						android:layout_height="50dp"
						android:background="?attr/selectableItemBackground"
						android:clickable="true"
						android:gravity="center_vertical"
						android:orientation="horizontal"
						android:paddingStart="20dp"
						android:paddingEnd="20dp">
						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="@string/Lbl_About"
							android:textSize="16sp"
							android:textStyle="normal"
							android:layout_gravity="start|center" />
						<ImageView
							android:layout_width="25dp"
							android:layout_height="25dp"
							android:src="@xml/ic_arrow_right"
							android:tint="?attr/colorSecondaryExtra"
							android:padding="2dp"
							android:layout_gravity="end|center" />
					</FrameLayout>
					<View
						android:layout_width="match_parent"
						android:layout_height="2dp"
